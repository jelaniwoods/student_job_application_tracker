<tr>
  <td>
    <%= application.title %>
  </td>

  <td>
    <%= application.company %>
  </td>

  <td>
    <%= application.due_on %>
  </td>

  <td>
    <% if application.submitted %>
      <i class="fas fa-check-square text-success"></i>
    <% else %>
      <i class="fas fa-times text-danger"></i>
    <% end %>
  </td>


  <td>
    <div class="btn-group">
      <%= link_to application do %>
        See details
      <% end %>
    </div>
  </td>
</tr>
